{
  "name": "back-end",
  "version": "1.0.0",
  "description": "Trabajo Práctico Profesional, FIUBA, 2020",
  "contributors": [
    "Sebastian Blanco <sebastian.e.blanco@gmail.com>",
    "Dylan Alvarez <dylanalvarez1995@gmail.com>",
    "Manuel Llauró <llauromanuel@gmail.com>"
  ],
  "engines": {
    "node": "12.14.0"
  },
  "main": "index.ts",
  "dependencies": {
    "@types/bluebird": "3.5.29",
    "@types/express": "4.17.2",
    "@types/glob": "7.1.1",
    "@types/graphql": "14.5.0",
    "@types/graphql-depth-limit": "1.1.2",
    "@types/node": "13.1.4",
    "@types/node-fetch": "2.5.4",
    "@types/sequelize": "4.28.8",
    "@types/validator": "12.0.1",
    "apollo-server": "2.9.15",
    "apollo-server-express": "2.9.15",
    "apollo-server-testing": "2.9.16",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "cpy-cli": "3.0.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "glob": "7.1.6",
    "graphql": "14.5.8",
    "graphql-depth-limit": "1.1.0",
    "graphql-import": "0.7.1",
    "graphql-import-node": "0.0.4",
    "http-status-codes": "1.4.0",
    "husky": "3.1.0",
    "link-module-alias": "1.2.0",
    "lodash": "4.17.15",
    "node": "12.14.0",
    "nodemon": "2.0.2",
    "pg": "7.17.0",
    "pg-hstore": "2.3.3",
    "reflect-metadata": "0.1.13",
    "sequelize": "5.21.3",
    "sequelize-cli-typescript": "3.2.0-c",
    "sequelize-typescript": "1",
    "typescript-logging": "0.6.4"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@types/jest": "24.0.25",
    "@types/supertest": "2.0.8",
    "babel-cli": "6.26.0",
    "babel-jest": "24.9.0",
    "babel-preset-env": "1.7.0",
    "concurrently": "3.5.0",
    "coveralls": "3.0.9",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "superagent": "5.1.3",
    "supertest": "4.0.2",
    "ts-jest": "24.2.0",
    "ts-node": "8.5.4",
    "tslint": "5.18.0",
    "tslint-config-fiuba-laboral-v2": "1.0.3",
    "typescript": "3.7.4"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"nodemon src/index.ts\" || true",
    "debug": "yarn build && concurrently --kill-others \"yarn watch:build\" \"nodemon --inspect src/index.ts\" || true",
    "build": "yarn clean && tsc -p . && yarn build:copy:files",
    "watch:build": "yarn build:copy:files && tsc -w -p . ",
    "build:copy:files": "cpy --cwd=src --parents '**/*' '!**/*.ts' ../dist/src",
    "start": "nodemon src/index.ts",
    "test": "jest --coverage --runInBand",
    "lint:src": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'test/**/*.ts'",
    "lint": "yarn lint:src && yarn lint:test",
    "preinstall": "which link-module-alias && link-module-alias clean || echo",
    "postinstall": "link-module-alias",
    "stash": "git stash -u --keep-index",
    "unstash": "git stash pop",
    "check-unstaged": "bash scripts/check_unstaged.sh",
    "clean": "rm -rf dist",
    "coveralls": "jest --coverage && cat ./tests/coverage/lcov.info | coveralls",
    "db:migrate": "yarn build && node_modules/.bin/sequelize db:migrate",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:drop": "node_modules/.bin/sequelize db:drop",
    "db:test:migrate": "yarn build && NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "db:test:create": "NODE_ENV=test node_modules/.bin/sequelize db:create",
    "db:test:drop": "NODE_ENV=test node_modules/.bin/sequelize db:drop",
    "pm2:start": "yarn build && pm2 startOrRestart --no-daemon src/config/process.yml --env staging",
    "db:start": "docker-compose up -d fiuba-laboral-v2-backend-database-postgres",
    "db:up": "docker-compose up fiuba-laboral-v2-backend-database-postgres",
    "db:stop": "docker-compose stop fiuba-laboral-v2-backend-database-postgres"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiuba-laboral-v2/back-end.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://fiuba-laboral-v2.atlassian.net"
  },
  "homepage": "https://github.com/fiuba-laboral-v2/back-end#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-unstaged && yarn lint && yarn test"
    }
  },
  "_moduleAliases": {
    "$root": ".",
    "$package": "package.json",
    "$models": "dist/src/models",
    "$src": "dist/src",
    "$libs": "dist/src/libs",
    "$graphql": "dist/src/graphql",
    "$test": "test",
    "$config": "dist/src/config"
  }
}
