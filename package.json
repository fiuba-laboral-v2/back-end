    "graphql-middleware": "4.0.2",
    "graphql-shield": "7.2.6",
    "http-status-codes": "1.4.0",
    "husky": "3.1.0",
    "isomorphic-fetch": "3.0.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.19",
    "moment": "2.29.1",
    "node": "14.15.0",
    "nodemon": "2.0.4",
