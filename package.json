    "@types/validator": "12.0.1",
    "apollo-error-converter": "1.1.0",
    "apollo-server": "2.14.2",
    "apollo-server-express": "2.14.2",
    "apollo-server-testing": "2.9.16",
    "bcrypt": "5.0.0",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.5",
    "core-js": "3.6.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-jwt": "6.0.0",
    "fast-xml-parser": "3.17.4",
    "fetch-mock": "9.10.5",
    "glob": "7.1.6",
    "graphql": "14.5.8",
    "graphql-depth-limit": "1.1.0",
